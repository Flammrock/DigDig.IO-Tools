{
  "name": "digdig-workspace",
  "private": true,
  "version": "1.0.0",
  "description": "digdig.io userscript workspace",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:custom-font && npm run build:userscript-plugin && npm run build:shared && npm run build:injection && npm run build:map-scanner-shared && npm run build:map-scanner-script && npm run build:map-scanner-tiny-viewer && npm run build:map-scanner-viewer",
    "build:userscript-plugin": "cd packages/userscript-webpack-plugin && npm run build",
    "build:shared": "cd packages/shared && npm run build",
    "build:injection": "cd packages/injection && npm run build",
    "build:custom-font": "cd packages/custom-font && npm run build",
    "build:map-scanner-shared": "cd packages/map-scanner-shared && npm run build",
    "build:map-scanner-script": "cd packages/map-scanner-script && npm run build",
    "start:map-scanner-tiny-viewer": "cd packages/map-scanner-tiny-viewer && npm start",
    "build:map-scanner-tiny-viewer": "cd packages/map-scanner-tiny-viewer && npm run build",
    "start:map-scanner-viewer": "cd packages/map-scanner-viewer && npm start",
    "build:map-scanner-viewer": "cd packages/map-scanner-viewer && npm run make"
  },
  "keywords": [
    "digdig.io",
    "script",
    "userscript"
  ],
  "author": "Flammrock",
  "license": "MIT"
}
