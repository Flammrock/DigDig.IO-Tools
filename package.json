{
  "name": "digdig-workspace",
  "private": true,
  "version": "1.0.2",
  "description": "digdig.io userscript workspace",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:custom-font && yarn run build:userscript-plugin && yarn run build:shared && yarn run build:injection && yarn run build:map-scanner-shared && yarn run build:map-scanner-script && yarn run build:map-scanner-tiny-viewer && yarn run build:map-scanner-viewer",
    "build:userscript-plugin": "cd packages/userscript-webpack-plugin && yarn run build",
    "build:shared": "cd packages/shared && yarn run build",
    "build:injection": "cd packages/injection && yarn run build",
    "build:custom-font": "cd packages/custom-font && yarn run build",
    "build:map-scanner-shared": "cd packages/map-scanner-shared && yarn run build",
    "build:map-scanner-script": "cd packages/map-scanner-script && yarn run build",
    "start:map-scanner-tiny-viewer": "cd packages/map-scanner-tiny-viewer && yarn start",
    "build:map-scanner-tiny-viewer": "cd packages/map-scanner-tiny-viewer && yarn run build",
    "start:map-scanner-viewer": "yarn workspace map-scanner-viewer start",
    "build:map-scanner-viewer": "yarn workspace map-scanner-viewer make"
  },
  "keywords": [
    "digdig.io",
    "script",
    "userscript"
  ],
  "author": "Flammrock",
  "license": "MIT"
}
